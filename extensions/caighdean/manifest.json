{
    "manifest_version": 2,
    "name": "L10n-tools-caighdean",
    "description": "The Caighdean Machine Translation engine",
    "version": "1.0",
    "default_locale": "en",
    "applications": {
        "gecko": {
            "id": "caighdean@l10n.mozilla.com",
            "strict_min_version": "54.0a1"
        }
