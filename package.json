{
  "name": "l10n-tools",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack -w -v --display-error-details --progress --colors",
    "start": "web-ext run -s extension/",
    "test-coverage": "jest",
    "test": "jest --collectCoverage false"
  },
  "author": "",
  "license": "MPL-2.0",
  "jest": {
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/web-ext-plugins/src/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./tests/js/__setup__/chrome.js"
    ],
    "testMatch": [
      "**/l10n-tools/tests/js/(tools|checks|addon|manager)/**/*.js?(x)",
      "**/extensions/**/tests/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "**/l10n-tools/tests/js/(tools|checks|addon|manager)/**/*.js?(x)",
      "**/l10n-tools/src/**/*.js?(x)",
      "**/l10n-tools/extensions/**/*.js?(x)",
      "!**/*.eslintrc.js",
      "!**/webpack.config.js",
      "!**/web-ext-plugins/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "6.2.9",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "6.18.0",
    "codecov": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-react": "^7.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "jest-webextension-mock": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.10",
    "react-table": "^6.7.6",
    "react-tabs": "^2.2.1",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.1",
    "web-ext": "^2.2.2",
    "web-extensions": "^0.2.0",
    "webpack": "1.14.0",
    "xml2js": "^0.4.19"
  },
  "dependencies": {
    "ajv": "^5.5.2",
    "character-entities": "^1.2.1",
    "child_process": "^1.0.2",
    "common-errors": "^1.0.5",
    "eslint": "^4.14.0",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "jsonschema": "^1.2.2",
    "l10n": "^0.2.4",
    "languages": "0.1.3",
    "marked": "^0.3.9",
    "pouchdb": "^6.4.1",
    "react-markdown": "^3.1.3",
    "sandbox": "^0.8.6",
    "unicode": "^10.0.0",
    "xregexp": "^3.2.0"
  }
}
